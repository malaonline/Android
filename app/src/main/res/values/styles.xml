<resources>


    <style name="ClubToolbar" parent="Widget.AppCompat.Toolbar">
        <item name="contentInsetStart">0dp</item>
    </style>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_blue_8fbcdd</item>
        <item name="colorPrimaryDark">@color/color_transparent_ffffff_00</item>
        <item name="colorAccent">@color/color_blue_8fbcdd</item>
        <item name="selectableItemBackgroundGeneral">@drawable/selectable_item_background_general_light</item>
        <item name="android:listDivider">@drawable/dividerdecoration</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="ShareTheme.notAnimation" parent="android:Theme.Translucent.NoTitleBar">
        <item name="android:windowAnimationStyle">@style/notAnimation</item>
    </style>
    <style name="notAnimation">
        <item name="android:activityOpenEnterAnimation">@null</item>
        <item name="android:activityOpenExitAnimation">@null</item>
        <item name="android:activityCloseEnterAnimation">@null</item>
        <item name="android:activityCloseExitAnimation">@null</item>
        <item name="android:taskOpenEnterAnimation">@null</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@null</item>
        <item name="android:taskToFrontEnterAnimation">@null</item>
        <item name="android:taskToFrontExitAnimation">@null</item>
        <item name="android:taskToBackEnterAnimation">@null</item>
        <item name="android:taskToBackExitAnimation">@null</item>
    </style>


    <style name="AppTheme.NoTitle">
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="AppWidget" parent="Theme.AppCompat.Light" />

    <style name="AppWidget.ShadowGap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/elevation_normal</item>
        <item name="android:layout_marginBottom">@dimen/shadow_gap_margin_fill</item>
        <item name="android:background">@drawable/gap_shadow</item>
    </style>

    <style name="AppWidget.DeepLine">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="mala.widget.usercenter.title.text" parent="android:Widget.TextView">
        <item name="android:textColor">#333</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="mala.widget.title.text.small" parent="android:Widget.TextView">
        <item name="android:textColor">#333</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="mala.widget.title.text.smallest" parent="android:Widget.TextView">
        <item name="android:textColor">#333</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="mala.widget.course.title.attr" parent="android:Widget.TextView">
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">@color/color_blue_88bcde</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="mala.widget.title.text.smaller" parent="android:Widget.TextView">
        <item name="android:textColor">#333</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="mala.widget.title.sub.text.smaller" parent="android:Widget.TextView">
        <item name="android:textColor">#6c6c6c</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="mala.widget.title.sub.text.mid" parent="android:Widget.TextView">
        <item name="android:textColor">#6c6c6c</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="mala.widget.course.title.text" parent="android:Widget.TextView">
        <item name="android:textColor">#939393</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="mala.widget.usercenter.warn.text" parent="android:Widget.TextView">
        <item name="android:textColor">#e26254</item>
        <item name="android:textSize">11sp</item>
    </style>

    <style name="mala.widget.usercenter.button" parent="android:Widget.Button">
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@drawable/core__btn_post_verify_code_bk</item>
    </style>

    <style name="mala.widget.usercenter.edit" parent="android:Widget.EditText">
        <item name="android:textColor">#6c6c6c</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColorHint">#cecece</item>
    </style>

    <style name="AppWidget.ItemDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/item_divider_height</item>
        <item name="android:background">@color/color_white_f6f6f6</item>
    </style>

    <style name="AppWidget.ItemLine">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/color_gray_e5e5e5</item>
    </style>

    <style name="AppWidget.ItemStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/color_white_ffffff</item>
        <item name="android:paddingBottom">@dimen/teacher_body_padding_top_normal</item>
        <item name="android:paddingLeft">@dimen/teacher_body_padding_normal</item>
        <item name="android:paddingRight">@dimen/teacher_body_padding_normal</item>
        <item name="android:paddingTop">@dimen/teacher_body_padding_top_normal</item>
    </style>

    <style name="AppWidget.LinearItemStyle" parent="AppWidget.ItemStyle">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="AppWidget.listLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/color_white_ffffff</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingBottom">@dimen/teacher_body_prices_padding_bottom</item>
        <item name="android:paddingLeft">@dimen/teacher_body_padding_normal</item>
        <item name="android:paddingRight">@dimen/teacher_body_padding_normal</item>
        <item name="android:paddingTop">@dimen/teacher_body_padding_top_normal</item>
    </style>


    <!-- activity进入退出动画 -->
    <style name="AppTheme.Animation" parent="AppTheme.NoActionBar">
        <item name="android:activityOpenEnterAnimation">@anim/push_left_in</item>
        <item name="android:activityOpenExitAnimation">@anim/push_left_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/push_right_in</item>
        <item name="android:activityCloseExitAnimation">@anim/push_right_out</item>
    </style>

    <style name="AppTheme.AnimationFullscreen" parent="AppTheme.Animation">

        <item name="android:windowNoTitle">true</item>
        <!-- Hides the Action Bar -->
        <item name="android:windowFullscreen">true</item>
        <!-- Hides the status bar -->
    </style>

    <style name="AppWidget.CardViewStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/color_gray_ededed</item>
    </style>

    <style name="CardView.LinearStyle">
        <item name="android:background">@color/color_white_ffffff</item>
        <item name="android:layout_marginLeft">@dimen/item_margin_normal</item>
        <item name="android:layout_marginRight">@dimen/item_margin_normal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="mala.widget.school.item" parent="android:Widget.TextView">
        <item name="android:background">@color/color_white_ffffff</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/toolbar_height</item>
    </style>

    <style name="mala.widget.school.item.text" parent="android:Widget.TextView">
        <item name="android:textColor">@color/color_black_333333</item>
        <item name="android:textSize">@dimen/text_size_normal_high</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginLeft">@dimen/layout_margin_top</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/title_bar_height</item>
    </style>

    <!-- 用户中心 -->
    <style name="mala.widget.usercenter.item.text" parent="android:Widget.TextView">
        <item name="android:textColor">@color/color_black_636363</item>
        <item name="android:textSize">@dimen/text_size_normal</item>
    </style>

    <style name="mala.widget.usercenter.item.text2" parent="android:Widget.TextView">
        <item name="android:textColor">@color/color_gray_d4d4d4</item>
        <item name="android:textSize">@dimen/text_size_small_high</item>
    </style>

    <!-- item间分割线 -->
    <style name="mala.widget.item.line" parent="android:Widget.TextView">
        <item name="android:background">@color/color_gray_f2f2f2</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
    </style>

    <!-- item风格 -->
    <style name="mala.widget.user.item" parent="android:Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <!-- 评价星 -->
    <style name="mala.ratingbar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingbar_drawable</item>
        <item name="android:minHeight">40dip</item>
        <item name="android:maxHeight">40dip</item>
    </style>

    <!-- 评价星 -->
    <style name="mala.ratingbar_small" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingbar_drawable_small</item>
        <item name="android:minHeight">13dip</item>
        <item name="android:maxHeight">13dip</item>
    </style>

    <style name="AppWidget.indicator" >
        <item name="ci_animator">@anim/indicator_no_animator</item>
        <item name="ci_drawable">@drawable/selected_indicator_radius</item>
        <item name="ci_drawable_unselected">@drawable/unselected_indicator_radius</item>
    </style>

    <style name="mala.indicator_small" parent="AppWidget.indicator">
        <item name="ci_width">6dp</item>
        <item name="ci_height">6dp</item>
        <item name="ci_margin">3dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">6dp</item>
    </style>

    <style name="mala.indicator_big" parent="AppWidget.indicator">
        <item name="ci_width">10dp</item>
        <item name="ci_height">10dp</item>
        <item name="ci_margin">5dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">10dp</item>
    </style>

    <style name="LiveCoursePriceStyle">
        <item name="android:textSize">@dimen/text_size_xxlarge</item>
        <item name="android:textColor">@color/color_red_e26254</item>
    </style>

    <style name="LiveCourseStuNum">
        <item name="android:textSize">@dimen/text_size_normal</item>
        <item name="android:textColor">@color/color_red_e26254</item>
    </style>

    <style name="Widget.GifMoviewView" parent="@android:style/Widget"></style>

    <style name="LiveCourse">
        <item name="android:padding">12dp</item>
        <item name="android:background">@color/color_gray_ededed</item>
    </style>

    <style name="LiveCourse.Card">
        <item name="android:background">@color/color_white_ffffff</item>
    </style>

    <style name="LiveCourse_Card_title">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#9bc3e1</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:layout_marginBottom">6dp</item>
    </style>
    <style name="LiveCourse_Card_line">
        <item name="android:layout_marginRight">-6dp</item>
        <item name="android:layout_marginLeft">-6dp</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:layout_marginBottom">6dp</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/color_gray_dadada</item>
    </style>
    <style name="LiveCourse_Card_content">
        <item name="android:textSize">@dimen/text_size_normal</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:layout_marginBottom">6dp</item>
    </style>

    <style name="LiveCourse_button">
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="schedule_text_style">
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/text_size_small_high</item>
        <item name="android:textColor">@color/color_black_6c6c6c</item>
    </style>

    <style name="schedule_icontext_style" parent="schedule_text_style">
        <item name="android:drawablePadding">5dp</item>
    </style>


    <!--文本style  父类-->
    <style name="mala_text_style_large_black_darker">
        <item name="android:textColor">@color/color_black_333333</item>
        <item name="android:textSize">@dimen/text_size_normal_high</item>
    </style>

    <style name="mala_text_style_large_black">
        <item name="android:textColor">@color/color_black_636363</item>
        <item name="android:textSize">@dimen/text_size_normal_high</item>
    </style>

    <style name="mala_text_style_large_black_light">
        <item name="android:textColor">@color/color_black_939393</item>
        <item name="android:textSize">@dimen/text_size_normal_high</item>
    </style>

    <style name="mala_text_style_normal_black_darker">
        <item name="android:textColor">@color/color_black_333333</item>
        <item name="android:textSize">@dimen/text_size_small_high</item>
    </style>

    <style name="mala_text_style_normal_black">
        <item name="android:textColor">@color/color_black_636363</item>
        <item name="android:textSize">@dimen/text_size_small_high</item>
    </style>

    <style name="mala_text_style_normal_black_light">
        <item name="android:textColor">@color/color_black_939393</item>
        <item name="android:textSize">@dimen/text_size_small_high</item>
    </style>


    <style name="order_detail_button" >
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/color_white_ffffff</item>
        <item name="android:textSize">@dimen/text_size_normal_high</item>
    </style>


    <style name="order_detail_card">
        <item name="android:padding">12dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:background">@color/color_white_ffffff</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="order_detail_card_title" parent="mala_text_style_large_black_darker">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
    </style>

    <style name="order_detail_card_item">
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="order_detail_card_text_black_dark" parent="mala_text_style_normal_black_darker">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="order_detail_card_text_black" parent="mala_text_style_normal_black">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="order_detail_card_text_black_light" parent="mala_text_style_normal_black_light">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="qr_code_tab_text" parent="@android:style/TextAppearance">
        <item name="android:textSize">@dimen/text_size_normal_high</item>
    </style>
</resources>
